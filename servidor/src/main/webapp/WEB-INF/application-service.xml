<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	   http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="clienteService" class="diseno.sistemas.negocio.mantenimiento.impl.ClienteServiceImpl">
		<property name="clienteDAO" ref="clienteDAO"></property>
	</bean>
	
	<bean id="loginService" class="diseno.sistemas.negocio.mantenimiento.impl.LoginServiceImpl">
		<property name="loginDAO" ref="loginDAO"></property>
	</bean>

	<bean id="datosService" class="diseno.sistemas.negocio.mantenimiento.impl.DatosServiceImpl">
		<property name="datosDAO" ref="datosDAO"></property>
	</bean>
	
	<bean id="productoService" class="diseno.sistemas.negocio.mantenimiento.impl.ProductoServiceImpl">
		<property name="productoDAO" ref="productoDAO"></property>
	</bean>
</beans>